        obj[0,i] = x;
        obj[1,i] = y;
    }
    if obj[4,i] == true
    {
        obj[2,i] = w0 + (w1 * -sin (obj[3,i] * pi / 180));
        var d = sqrt(sqr(obj[0,i] - px) + sqr(obj[1,i] - py));
