            <string>if timer&gt;-60{

display_set_gui_size(1280,960);
draw_set_alpha(1);
draw_set_font(font_numbers);
draw_set_halign(fa_left);
if spell_timer &gt; 600
    {
    draw_set_color(make_color_rgb(200, 200, 255));
    timeout_snd = 0;
    }
else if spell_timer &gt; 300
    {
    draw_set_color(make_color_rgb(255, 100, 100));
    timeout_snd = 1;
    }
else
    {
    draw_set_color(make_color_rgb(255, 0, 0));
    timeout_snd = 2;
    }
draw_text(780,55,real(ceil(spell_timer/60)));

if spellcard_on == true
    {
    draw_set_color(c_white);
    draw_set_font(font_number2);
    draw_text(720,95,string("Bonus: ") + string(round(spell_bonus)));
    }

}

//Name
display_set_gui_size(1280,960);
draw_set_alpha(alpha);
draw_set_font(font_menu);
draw_set_halign(fa_left);
draw_set_color(c_white);
draw_text(name_x,55,string(name));


//Boss Debug
if global.debug==true
    {
    draw_set_alpha(1);
    draw_set_font(font_debug);
    draw_text(700,120,"Boss Debug");
    draw_text(700,140,"Timer: " + string(timer));
    draw_text(700,160,"Spellcard: " + string(spellcard));
    draw_text(700,180,"Health: " + string(hp));
    draw_text(700,200,"X: " + string(x));
    draw_text(700,220,"Y: " + string(y));
    }

