        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>timer+=1; // Timer for looping spell cards

if timeout_snd == 1
    {
    if frac(spell_timer/60) == 0
        {audio_play_sound(sfx_menu_togame,1,false);}
    }
else if timeout_snd == 2
    {
    if (frac(spell_timer/60) == 0 || frac(spell_timer/60) == .90) &amp;&amp; timer &gt; 0
        {audio_play_sound(sfx_menu_togame,1,false);}
    }

if spell_timer&gt;0 &amp;&amp; timer&gt;0 {spell_timer-=1;} // Spellcard Timer

if spell_timer==0 || hp&lt;0  // Next Spellcard
    {
    spell_timer = -1;
    spellcard+=1;
    spell_start=true;
    global.clear=true;
    spellcard_on = false;
    // Spell card get ~desu!!!!
    if spell_get==true &amp;&amp; spell_timer &gt; 0
        {
        spell_bonus = round(spell_bonus);
        with instance_create(0,0,obj_spell_bonus)
            {bonus = other.spell_bonus;}
        global.score += spell_bonus;
        spell_get = false;
        global.star=true;
        }
    else
        {global.star=false;}
    }

// Spell Bonus
if timer &gt; 0 &amp;&amp; spell_bonus &gt; 0
    {
    spell_bonus -= spell_bonus_start / spell_timer_start
    }

// Save boss position
global.enemy_x=x;
global.enemy_y=y;

// Collision &amp; Take Damage
scr_boss_collision();

if global.bomb=true || global.player_death==true
    {spell_get=false;}

// For name
if alpha&lt;1 {alpha+=0.025; name_x-=1.25;}

// Hover Effect
yDir += 2 * global.gamespeed;
yDraw = lengthdir_y(5,yDir);
</string>
          </argument>
        </arguments>
      </action>
