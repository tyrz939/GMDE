    frames = 30;
    dir = 1
    y_mov = tmp - ypos;
    y_start = ypos;
}
if dir != 0 {
    ypos = y_start + (y_mov * sin (dir * pi / 180));
    dir += 90 / frames;
    if dir &gt;= 90 {
        dir = 0;
    }
}
