            <string>if delay==0{
scr_pause_menu_step();

var push = 0;
push = max(input.shoot_press,0);

if input.esc{ // Esc Resume
    instance_destroy();
    global.pause=0;
    audio_resume_all();
    exit;
}

if push == 1{
    audio_stop_sound(sfx_menu_sel);
    audio_play_sound(sfx_menu_sel,1,false);
    switch mpos{
        case 0: // Resume
            if global.continues &gt; 0{
                instance_destroy();
                global.pause = 0;
                global.life = 2;
                global.continues--;
                global.game_over = false;
                global.clear = true;
                global.pow = global.max_power;
                global.score = 0;
                global.can_save = false;
                obj_game.deadTimer = 30;
                audio_resume_all();
            }
            break;
        case 1: // Replay Save
            if global.can_save == true{
                instance_create(100,100,obj_confirm_rpl)
                delay = 1;
                instance_deactivate_object(self);
            }
            break;
        case 2: // Quit to Menu
            instance_create(100,100,obj_confirm_quit)
            delay = 1;
            instance_deactivate_object(self);
            break;
    }
}

} else {delay-=1;}
</string>
