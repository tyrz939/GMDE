if file_exists(working_directory + string(global.rpl)){
    file = file_bin_open(working_directory + string(global.rpl), 0);
    // Difficulty, Character
    file_bin_read_byte(file);
    file_bin_read_byte(file);
    // Skip Date
    var date_string = file_bin_read_byte(file)+1;
    file_bin_seek(file, file_bin_position(file) + date_string);
    // Skip Time
    var time_string = file_bin_read_byte(file)+1;
    file_bin_seek(file, file_bin_position(file) + time_string);
    // Level &amp; Position in level
    file_bin_read_byte(file);
    file_bin_read_byte(file);
    
    // What points you can load from
    var states0 = file_bin_read_byte(file);
    if states0 &gt;= 128 {gray[0] = false; states0 -= 128;} else {gray[0] = true;}
    if states0 &gt;= 64 {gray[1] = false; states0 -= 64;} else {gray[1] = true;}
    if states0 &gt;= 32 {gray[2] = false; states0 -= 32;} else {gray[2] = true;}
    if states0 &gt;= 16 {gray[3] = false; states0 -= 16;} else {gray[3] = true;}
    if states0 &gt;= 8 {gray[4] = false; states0 -= 8;} else {gray[4] = true;}
    if states0 &gt;= 4 {gray[5] = false; states0 -= 4;} else {gray[5] = true;}
    if states0 &gt;= 2 {gray[6] = false; states0 -= 2;} else {gray[6] = true;}
    if states0 &gt;= 1 {gray[7] = false; states0 -= 2;} else {gray[7] = true;}
    var states1 = file_bin_read_byte(file);
    if states1 &gt;= 32 {gray[8] = false; states1 -= 32;} else {gray[8] = true;}
    if states1 &gt;= 16 {gray[9] = false; states1 -= 16;} else {gray[9] = true;}
    if states1 &gt;= 8 {gray[10] = false; states1 -= 8;} else {gray[10] = true;}
    if states1 &gt;= 4 {gray[11] = false; states1 -= 4;} else {gray[11] = true;}
    if states1 &gt;= 2 {gray[12] = false; states1 -= 2;} else {gray[12] = true;}
    if states1 &gt;= 1 {gray[13] = false; states1 -= 2;} else {gray[13] = true;}

}

for(var i = 0; i &lt; 13; i++){
    if gray[i] == false{
        mpos = i;
        break;
    }
}
