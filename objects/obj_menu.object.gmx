        <arguments>
          <argument>
            <kind>1</kind>
            <string>scr_menu_step();

if input.bomb_press &amp;&amp; mpos != 4
{
    mpos = array_length_1d(menu)-1;
    audio_stop_sound(sfx_menu_move);
    audio_play_sound(sfx_menu_move,1,false);
}

var push = 0;
push = max(input.shoot_press,0);

if push == 1
{
    audio_stop_sound(sfx_menu_sel);
    audio_play_sound(sfx_menu_sel,1,false);
    switch mpos
    {
        case 0: // Start Game
            global.demo_play = false;
            global.replay = false;
            global.level = obj_stage1;
            global.start_game = true;
            scr_start(); // Loads variables
            instance_deactivate_object(self);
            instance_create(x,y,obj_diff_sel);
            break;
        case 1: // Start Extra
            global.demo_play = false;
            global.replay = false;
            global.difficulty = 3;
            global.level = obj_extra;
            global.start_game = true;
            scr_start(); // Loads variables
            instance_deactivate_object(self);
            room_goto(room_ingame);
            break;
        case 2: // Replay Menu
            global.demo_play = false;
            global.start_game = true;
            scr_start(); // Loads variables
            instance_deactivate_object(self);
            instance_create(x,y,obj_replay_menu);
            break;
        case 3: // Options Menu
            instance_deactivate_object(self);
            instance_create(x,y,obj_options_menu);
            break;
        case 4: // Quit
            global.quit=true;
            scr_save();
            break;
    }
}

// Demo Play time out
if input.up || input.down || input.left || input.right || input.focus || input.shoot_press || input.bomb_press || input.esc
    {timer = 0;}
timer++;
if timer == timeout
{
    global.rpl = string("\demo" + string(choose(1,2,3)) + ".rpl");
    if file_exists(working_directory + string(global.rpl))
    {
        global.replay = true;
        global.demo_play = true;
        room_goto(room_ingame);
    }
}
</string>
