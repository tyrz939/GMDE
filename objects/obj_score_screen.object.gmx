counter=0;
timer=0;
duration=120;

//Title
if global.game_over==true
    {title="Game Over";}
else
    {title="Congratulation!!";}

if global.game_over==true // Clear Bonus
    {clear="None";}
else
    {clear=2000000 * (global.difficulty+1); counter+=clear;}

if global.graze &gt; 0
    {graze = global.graze * 100; counter+=graze;} // Graze
else
    {graze = "None";}

if global.game_over==true // Lives
    {life="None";}
else
    {life=global.life*200000; counter+=life;}

if global.point &gt; 0
    {point = global.point * 200; counter+= point;}
else
    {point = "None";}
counterDraw=counter;

counter=counter/duration;
